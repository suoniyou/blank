.main-container{
  min-height: 95vh;
  background-color: #f5f5f5;
  padding: 24rpx;
  box-sizing: border-box;
}
.base-box {
  /* width: 702rpx; */
  /* min-height: 10rpx; */
  background: #fff;
  border-radius: 24rpx;
  margin-bottom: 16rpx;
  padding: 24rpx;
  box-sizing: border-box;
}
.small-title {
  font-size: 28rpx;
  color: #1a1a1a;
  font-weight: bold;
}
.withdraw-wrap {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 14rpx 0;
  border-bottom: 0.5px solid #f5f5f5
}
.input-wrap {
  width: 50%;
  position: relative;
}
.withdraw-input {
  width: 100%;
  color: #1a1a1a;
  height: 84rpx;
  padding: 0 48rpx;
}
.withdraw-input::before {
  height: 84rpx;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  content: "ï¿¥";
  color: #1a1a1a;
  font-size: 32rpx;
  font-weight: bold;
}
.all-in-box {
  text-align: right;
  width: 50%;

  }
  .all-in-text {
    color: #ff3212;
    font-size: 28rpx;
  }
  .remain-cost {
    margin-top: 5rpx;
    color: #b2b2b2;
    font-size: 22rpx;
  }
  .withdraw-condition {
    color: #b2b2b2;
    font-size: 24rpx;
    text-align: left;
    margin-top: 16rpx;
  }
  .account {
    height: 168rpx;
   }
   .account-title {
    font-size: 28rpx;
    color: #1a1a1a;
    font-weight: bold;
  }
  .user-wrap {
    display: flex;
